{
  "name": "@appwrite/sdk-project",
  "version": "15.0.0",
  "description": "Appwrite TypeScript SDK for Web and Node.js",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "appwrite",
    "sdk",
    "typescript",
    "javascript",
    "backend",
    "baas",
    "serverless",
    "api",
    "database"
  ],
  "author": "Appwrite Team <team@appwrite.io>",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/appwrite/appwrite.git",
    "directory": "public/sdk-project"
  },
  "bugs": {
    "url": "https://github.com/appwrite/appwrite/issues"
  },
  "homepage": "https://appwrite.io",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "cross-fetch": "^4.0.0",
    "node-fetch": "^3.3.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}